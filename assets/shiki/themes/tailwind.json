    "tokenColors": [
        {
            "name": "Shell Numbers and Version Strings",
            "scope": [
                "constant.numeric",
                "string",      
                "support.constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#1E40AF"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#94a3b8"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation"
            ],
            "settings": {
                "foreground": "#44403c"
            }
        },
        {
            "name": "Constant - eg. Number, Boolean, Colors",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#EC4899"
            }
        },
        {
            "name": "Entity - eg. Function, Type, Tag, Attribute Name",
            "scope": [
                "entity"
            ],
            "settings": {
                "foreground": "#758ca9"
            }
        },
        {
            "name": "Entity Name - eg. Function, Type, Tag, Attribute Name",
            "scope": [
                "entity.name"
            ],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "name": "Entity Name Function - eg. Function, Type, Tag, Attribute Name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#00bf99"
            }
        },
        {
            "name": "Keyword - eg. Control (while, for), Operator (or)",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#EC4899"
            }
        },
        {
            "name": "Keyword Assignment - eg. =",
            "scope": [
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#35393e"
            }
        },
        {
            "name": "Storage - eg. Type (class, function, var)",
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#3730a3"
            }
        },
        {
            "name": "String - eg. Quoted Value, Interpolated Values, Regex",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#1e40af"
            }
        },
        {
            "name": "Support - eg. Key in JSON",
            "scope": [
                "support"
            ],
            "settings": {
                "foreground": "#EC4899"
            }
        },
        {
            "name": "Variable - eg. Named Import, Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#3730A3"
            }
        },
        {
            "name": "Variable Object - eg. Named Import, Variable",
            "scope": [
                "variable.other.constant.object"
            ],
            "settings": {
                "foreground": "#e11d48"
            }
        },
        {
            "name": "[HTML] - Entity",
            "scope": [
                "text.html entity"
            ],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "name": "[HTML] - Attribute Names",
            "scope": [
                "text.html entity.other.attribute-name",
                "text.html meta.tag"
            ],
            "settings": {
                "foreground": "#0ea5e9"
            }
        },
        {
            "name": "[HTML] - Attribute Values",
            "scope": [
                "text.html string.quoted"
            ],
            "settings": {
                "foreground": "#1E40AF"
            }
        },
        {
            "name": "[HTML] - Attribute Values Quotes",
            "scope": [
                "text.html punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#0ea5e9"
            }
        },
        {
            "name": "[HTML] - Punctuation",
            "scope": [
                "text.html punctuation",
                "text.html keyword.operator"
            ],
            "settings": {
                "foreground": "#0ea5e9"
            }
        },
        {
            "name": "[HTML] - Doctype",
            "scope": [
                "text.html meta.tag.sgml",
                "text.html meta.tag.sgml punctuation"
            ],
            "settings": {
                "foreground": "#5f7692"
            }
        },
        {
            "name": "[HTML] - Function",
            "scope": [
                "text.html entity.name.function"
            ],
            "settings": {
                "foreground": "#00bf99"
            }
        },
        {
            "name": "[CSS] - Entity - eg. .class, #id, body, :root",
            "scope": [
                "source.css entity",
                "source.css entity.other.attribute-name.class",
                "source.css entity.other.attribute-name.id",
                "source.css entity.other.attribute-name.pseudo-class",
                "source.css punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "name": "[CSS] - Meta Attribute - eg. [data-attribute]",
            "scope": [
                "source.css meta.attribute-selector",
                "source.css meta.attribute-selector punctuation.definition.string",
                "source.css entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#6366f1"
            }
        },
        {
            "name": "[CSS] - @ - eg. @media",
            "scope": [
                "source.css keyword.control.at-rule",
                "source.css punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#6366f1"
            }
        },
        {
            "name": "[CSS] - Meta Value - eg. value",
            "scope": [
                "source.css meta.property-value",
                "source.css meta.property-value.css punctuation.definition.constant",
                "source.css meta.property-list meta.at-rule",
                "source.css variable.parameter"
            ],
            "settings": {
                "foreground": "#075985"
            }
        },
        {
            "name": "[CSS] - Constant - eg. 2.55, ease-out",
            "scope": [
                "source.css constant",
                "source.css support.constant"
            ],
            "settings": {
                "foreground": "#324258"
            }
        },
        {
            "name": "[CSS] - Meta Name - eg. font-family, 'name'",
            "scope": [
                "source.css meta.property-name",
                "source.css support.type.property-name",
                "source.css string"
            ],
            "settings": {
                "foreground": "#2563eb"
            }
        },
        {
            "name": "[CSS] - Function - eg. var(), rgba()",
            "scope": [
                "source.css support.function"
            ],
            "settings": {
                "foreground": "#324258"
            }
        },
        {
            "name": "[CSS] - Keyword - eg. =, and",
            "scope": [
                "source.css keyword.control",
                "source.css keyword.operator"
            ],
            "settings": {
                "foreground": "#64748b"
            }
        },
        {
            "name": "[CSS] - Unit - eg. px, %",
            "scope": [
                "source.css keyword.other.unit"
            ],
            "settings": {
                "foreground": "#1E40AF"
            }
        },
        {
            "name": "[CSS] - Variable - eg. --var",
            "scope": [
                "source.css variable",
                "source.css entity.other.attribute-name.placeholder",
                "source.css entity.other.attribute-name.placeholder punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#db2777"
            }
        },
        {
            "name": "[CSS] - Braces - eg. {}",
            "scope": [
                "source.css punctuation.section"
            ],
            "settings": {
                "foreground": "#94a3b8"
            }
        },
        {
            "name": "[CSS] - Punctuation - eg. <> , . () []",
            "scope": [
                "source.css punctuation.separator",
                "source.css punctuation.terminator",
                "source.css punctuation.definition.constant",
                "source.css meta.attribute-selector punctuation.definition",
                "source.css punctuation.definition.parameters",
                "source.css punctuation.access",
                "source.css meta.property-list"
            ],
            "settings": {
                "foreground": "#6366F1"
            }
        },
        {
            "name": "[JSON] - Support",
            "scope": [
                "source.json support"
            ],
            "settings": {
                "foreground": "#3730a3"
            }
        },
        {
            "name": "[JSON] - Constant",
            "scope": [
                "source.json constant"
            ],
            "settings": {
                "foreground": "#EC4899"
            }
        },
        {
            "name": "[JSON] - Constant (Escaped) - eg. '",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#cbd5e1"
            }
        },
        {
            "name": "[JS] - Variable",
            "scope": [
                "variable.other.property.js"
            ],
            "settings": {
                "foreground": "#3730a3"
            }
        },
        {
            "name": "[JS] - Import",
            "scope": [
                "meta.import keyword.control.import"
            ],
            "settings": {
                "foreground": "#3730a3"
            }
        },
        {
            "name": "[JS] - Import From",
            "scope": [
                "meta.import keyword.control.from",
                "meta.import punctuation"
            ],
            "settings": {
                "foreground": "#44403c"
            }
        },
        {
            "name": "[JS] - Import String - eg. '../location/file",
            "scope": [
                "meta.import string",
                "meta.import string punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#3730a3"
            }
        },
        {
            "name": "[JS] - Import Variable - eg. React, Moment",
            "scope": [
                "meta.import variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#2563eb"
            }
        },
        {
            "name": "[JSON] - Property names (keys)",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name"
            ],
            "settings": {
                "foreground": "#1e40af"
            }
        },
        {
            "name": "[JSON] - Colon",
            "scope": [
                "source.json punctuation.separator.dictionary.key-value",
                "source.json punctuation.separator.array"
            ],
            "settings": {
                "foreground": "#94a3b8"
            }
        },
        {
            "name": "[JSON] - Brackets",
            "scope": [
                "source.json punctuation.definition.dictionary.begin",
                "source.json punctuation.definition.dictionary.end",
                "source.json punctuation.definition.array.begin",
                "source.json punctuation.definition.array.end"
            ],
            "settings": {
                "foreground": "#94a3b8"
            }
        }
    ],
    "semanticHighlighting": true
}
